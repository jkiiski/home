[user]
	name = Janne Kiiski
[color]
	ui = auto
[log]
	decorate = short
[push]
	default = tracking
[pull]
	rebase = true
[merge]
	tool = vimdiff
[rebase]
	autosquash = true
[alias]
	logg = log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold cyan)<%an>%Creset' --abbrev-commit --date=relative
